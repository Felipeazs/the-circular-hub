{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"incremental": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"jsx": "react-jsx",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"baseUrl": ".",
		"paths": {
			"@/client/*": ["./src/*"]
		},
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": ["src"]
}
